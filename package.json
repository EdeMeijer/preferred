{
  "name": "preferred",
  "version": "0.2.1",
  "description": "Utility for working with preferred number sequences",
  "main": "dist/preferred.js",
  "repository": "github:EdeMeijer/preferred",
  "scripts": {
    "build": "npm run build:prod && npm run build:min",
    "build:prod": "rollup -c",
    "watch": "rollup -c --watch",
    "build:min": "rollup -c --environment MINIFY",
    "test": "npm run build:prod && jest"
  },
  "keywords": [
    "preferred",
    "number"
  ],
  "homepage": "https://github.com/EdeMeijer/preferred",
  "author": "Ede Meijer",
  "license": "MIT",
  "devDependencies": {
    "jest": "^24.9.0",
    "jest-matcher-deep-close-to": "^1.3.0",
    "rollup-plugin-babel-minify": "^9.0.0"
  },
  "dependencies": {}
}
